{
    "headers": [
      {
        "source": "/api/r2upload",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "POST"
          },
          {
            "key": "Access-Control-Allow-Headers",
            "value": "Content-Type"
          }
        ]
      }
    ],
    "functions": {
      "/api/*.js": {
        "maxDuration": 120,
        "memory": 3008
      }
    }
  }